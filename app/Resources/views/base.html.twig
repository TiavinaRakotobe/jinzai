<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>{{ app_name }} | {% block title %}Gestion demande{% endblock %}</title>
        <!-- Tell the browser to be responsive to screen width -->
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bundles/app/css/bootstrap.min.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/font-awesome.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/bootstrap-datepicker3.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/all.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/ionicons.min.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/select2.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/AdminLTE.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/_all-skins.min.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/bootstrap-colorpicker.min.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/fullcalendar.min.css') }}"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/fullcalendar.print.min.css') }}" media="print"/>
            <link rel="stylesheet" href="{{ asset('bundles/app/css/conge.css') }}"/>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
{% block body %}{% endblock %}
