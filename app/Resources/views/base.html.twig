    <script src="{{ asset('bundles/app/js/fastclick.js') }}"></script>
    <script src="{{ asset('bundles/app/js/adminlte.min.js') }}"></script>
    <script src="{{ asset('bundles/app/js/bootstrap-colorpicker.min.js') }}"></script>
